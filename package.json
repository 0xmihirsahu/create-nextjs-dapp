{
  "name": "create-nextjs-dapp",
  "version": "0.1.0",
  "description": "Create a Next.js dApp with your preferred wallet provider",
  "bin": {
    "create-nextjs-dapp": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts src/helpers/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "test": "node --test test/cli.test.js test/helpers.test.js",
    "test:all": "node --test test/*.test.js",
    "test:build": "node --test test/build.test.js",
    "test:unit": "node --test test/helpers.test.js",
    "test:integration": "node --test test/cli.test.js"
  },
  "keywords": [
    "nextjs",
    "dapp",
    "web3",
    "ethereum",
    "rainbowkit",
    "privy",
    "dynamic",
    "reown",
    "wagmi",
    "cli"
  ],
  "author": "Mihir Sahu <0xmihirsahu@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xmihirsahu/create-nextjs-dapp.git"
  },
  "homepage": "https://github.com/0xmihirsahu/create-nextjs-dapp#readme",
  "bugs": {
    "url": "https://github.com/0xmihirsahu/create-nextjs-dapp/issues"
  },
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "picocolors": "^1.1.1",
    "update-notifier": "^7.3.1",
    "validate-npm-package-name": "^7.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.0",
    "@types/update-notifier": "^6.0.8",
    "@types/validate-npm-package-name": "^4.0.2",
    "eslint": "^9.18.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.20.0"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "templates"
  ]
}
